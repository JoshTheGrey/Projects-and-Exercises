package packages;

import java.util.*;

public class multiples {
	public static void main(String[] args) {
		System.out.println(threeM(560));
			
		}
	static ArrayList<Integer> threeM (int n) {
		int m = 0;
		int count = 0;
		int three = 3;
		
		ArrayList<Integer> threeM = new ArrayList<>();
		
			while (m <= n) {
				m = count* three;
				threeM.add(m);
				count +=1;
			}
			
		return threeM;
		}
	}
